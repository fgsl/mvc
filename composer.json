{
	"name" : "fgsl/mvc",
	"type" : "framework",
	"description" : "Extensions of Laminas MVC",
	"keywords" : [
		"framework",
		"mvc"
	],
	"homepage" : "https://github.com/fgsl/mvc",
	"license" : "AGPL-3.0-or-later",
	"autoload" : {
		"psr-4" : {
			"Fgsl\\" : "src/Fgsl"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Fgsl\\Test\\" : "tests/"
		}
	},
	"require" : {
		"php" : "^8.2",
		"fgsl/form" : "^2.0",
		"laminas/laminas-i18n-resources" : "^2.11.0",
		"laminas/laminas-mvc" : "^3.8.0",
		"laminas/laminas-mvc-i18n" : "^1.9.0",
		"laminas/laminas-mvc-plugin-flashmessenger" : "^1.11.0",
		"laminas/laminas-navigation" : "^2.20.0",
		"laminas/laminas-paginator" : "^2.19.0",
		"fgsl/db": "^2.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^10.5"		
	},
	"config" : {
		"secure-http" : false
	}
}
